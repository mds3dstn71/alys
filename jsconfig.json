{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "alys/*": [
        "./components/*"
      ],
      "@/*": [
        "./components/*"
      ],
      "@common/*": [
        "./components/common/*"
      ],
      "@utils/*": [
        "./components/utils/*"
      ]
    }
  },
  "include": [
    "scripts/**/*",
    "src/**/*"
  ]
}

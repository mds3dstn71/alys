{
  "name": "alys",
  "version": "0.1.0-beta.5",
  "description": "A minimal-configuration, \"drop-in\" CSS boilerplate",
  "main": "dist/alys.min.css",
  "scripts": {
    "lint": "stylelint \"css/**/*.css\"",
    "dev": "postcss css/alys.css -o dist/alys.min.css -c postcss.config.js -w",
    "build": "postcss css/alys.css -o dist/alys.min.css -c postcss.config.js",
    "start:testpage": "lite-server",
    "test": "npm run lint"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mds3dstn71/alys.git"
  },
  "keywords": [
    "CSS",
    "toolkit",
    "reset"
  ],
  "author": "Rodger Jordas <mds3dstn71@pm.me> (https://github.com/mds3dstn71)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mds3dstn71/alys/issues"
  },
  "homepage": "https://github.com/mds3dstn71/alys#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "cssnano": "^3.10.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "postcss": "^6.0.11",
    "postcss-apply": "^0.8.0",
    "postcss-cli": "^4.1.1",
    "postcss-color-function": "^4.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-font-family-system-ui": "^2.0.1",
    "postcss-import": "^11.0.0",
    "postcss-initial": "^2.0.0",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nesting": "^4.2.1",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.1.2",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0"
  },
  "dependencies": {
    "normalize.css": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ]
}

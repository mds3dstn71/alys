{
  "name": "alys-build-template",
  "version": "0.3.0",
  "description": "♠️ A tiny React component library",
  "keywords": [
    "React",
    "UI",
    "component",
    "library"
  ],
  "homepage": "https://alys.js.org",
  "bugs": {
    "url": "https://github.com/rmjordas/alys/issues"
  },
  "license": "Apache-2.0",
  "author": "Rodger Jordas <rmjordas@gmail.com> (https://rmjordas.github.io)",
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmjordas/alys.git"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx --ignore-path .eslintignore",
    "test": "node scripts/test.js",
    "gen:docs": "node scripts/generate-component-data",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "prestart": "npm run gen:docs",
    "start": "npm-run-all --parallel start:docs gen:docs-watch",
    "start:docs": "node scripts/start.js",
    "prebuild:docs": "npm run gen:docs",
    "build:docs": "node scripts/build.js",
    "build:cjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore **/*.spec.js",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm-run-all --parallel build:cjs build:css build:copy-files",
    "build:copy-files": "node scripts/copy-build-files"
  },
  "dependencies": {
    "dayjs": "1.8.5",
    "prismjs": "1.15.0",
    "prop-types": "15.6.2",
    "throttle-debounce": "2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.1.6",
    "@babel/preset-env": "7.3.1",
    "@babel/runtime": "7.3.1",
    "@svgr/webpack": "2.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "3.1.3",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-plugin-styled-components": "1.10.0",
    "babel-preset-react-app": "^7.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "chokidar": "2.1.0",
    "cpx": "1.5.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "eslint": "5.6.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "prettier": "1.16.4",
    "react": "16.8.1",
    "react-app-polyfill": "^0.2.0",
    "react-dev-utils": "^7.0.1",
    "react-docgen": "3.0.0",
    "react-dom": "16.8.1",
    "react-test-renderer": "16.8.1",
    "resolve": "1.8.1",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "styled-components": "4.1.3",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "peerDependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "styled-components": "^4.1.1"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "private": true
}

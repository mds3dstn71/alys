{
  "private": true,
  "name": "alys-build-template",
  "version": "0.3.2",
  "description": "♠️ A tiny React component library",
  "keywords": [
    "React",
    "UI",
    "component",
    "library"
  ],
  "homepage": "https://alys.js.org",
  "bugs": {
    "url": "https://github.com/rmjordas/alys/issues"
  },
  "license": "Apache-2.0",
  "author": "Rodger Jordas <rmjordas@gmail.com> (https://rmjordas.github.io)",
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmjordas/alys.git"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx --ignore-path .eslintignore",
    "test": "node scripts/test.js",
    "gen:docs": "node scripts/generate-component-data",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "prestart": "npm run gen:docs",
    "start": "npm-run-all --parallel start:docs gen:docs-watch",
    "start:docs": "node scripts/start.js",
    "prebuild:docs": "npm run gen:docs",
    "build:docs": "node scripts/build.js",
    "build:cjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore **/*.spec.js",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm-run-all --parallel build:cjs build:css build:copy-files",
    "build:copy-files": "node scripts/copy-build-files"
  },
  "dependencies": {
    "dayjs": "1.8.14",
    "prismjs": "1.16.0",
    "prop-types": "15.7.2",
    "throttle-debounce": "2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "7.4.3",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@ruj/prettier-config": "1.0.0",
    "@svgr/webpack": "4.1.0",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-react-app": "^9.0.0",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.1",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "mini-css-extract-plugin": "0.5.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "react-docgen": "^4.1.0",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "resolve": "1.10.0",
    "rimraf": "^2.6.3",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "styled-components": "^4.2.0",
    "terser-webpack-plugin": "1.2.3",
    "ts-pnp": "1.1.2",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "styled-components": "^4.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
